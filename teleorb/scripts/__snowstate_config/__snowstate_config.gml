/**
*	SnowState Configuration
*	Documentation: https://github.com/sohomsahaun/SnowState/wiki/Configuration
*/

// Whether to show warning messages in Output (true) or not (false)
#macro SNOWSTATE_DEBUG_WARNING true

// Whether history should be tracked by default (true) or not (false)
#macro SNOWSTATE_HISTORY_ENABLED false

// Default maxmimum history count
#macro SNOWSTATE_DEFAULT_HISTORY_MAX_SIZE 5

// Whether to throw an error when circular inheritance is found (true) or not (false)
#macro SNOWSTATE_CIRCULAR_INHERITANCE_ERROR true